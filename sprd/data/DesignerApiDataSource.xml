<data:DesignerApiDataSourceClass xmlns:data="sprd.data" xmlns:conf="js.conf">
    <conf:DataSourceConfiguration>
        <conf:ResourceConfiguration modelClassName="sprd.model.TemplateCategory" path="categories">
            <conf:ResourceConfiguration modelClassName="sprd.model.Template" path="templates"/>
        </conf:ResourceConfiguration>
        <conf:ResourceConfiguration modelClassName="sprd.model.AbstractShop" path="shops">
            <conf:ResourceConfiguration modelClassName="sprd.model.Transformer" path="text/transform" processor="TransformationProcessor"/>
        </conf:ResourceConfiguration>
        <conf:ResourceConfiguration modelClassName="sprd.model.Mask" path="masks">
            <conf:ResourceConfiguration modelClassName="sprd.model.MaskApplier" path="apply"/>
        </conf:ResourceConfiguration>

    </conf:DataSourceConfiguration>
</data:DesignerApiDataSourceClass>