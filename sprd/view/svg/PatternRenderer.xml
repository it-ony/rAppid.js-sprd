<sprdSvg:PatternRendererClass xmlns="http://www.w3.org/2000/svg" xmlns:sprdSvg="sprd.view.svg" xmlns:js="js.core">

    <js:Repeat items="{configuration.printColors}" >
        <js:Template name="item">
            <pattern id="p{maskId}-{index}" width="1" height="1" visible="{isSpecialColor(index)}">
                <image width="{largeSize}" height="{largeSize}" href="{patternUrl(index)}"/>
            </pattern>
        </js:Template>
    </js:Repeat>


    <filter id="g{maskId}" filterUnits="userSpaceOnUse">
        <feGaussianBlur stdDeviation="7.5"/>
        <feOffset dx="0.1" dy="-0.1" result="blur"/>
        <feFlood flood-color="rgb(210, 238, 166)" flood-opacity="1"/>
        <feComposite in2="blur" operator="in" result="colorShadow"/>
        <feComposite in="SourceGraphic" in2="colorShadow" operator="over"/>
    </filter>

    <image href="{baseUrl('sprd/css/loading-spin-white.svg')}" width="{loaderSize()}" height="{loaderSize()}"
           x="{loaderPos().x}" y="{loaderPos().y}" class="loader-white" visible="{loading}"/>
    <image href="{baseUrl('sprd/css/loading-spin-black.svg')}" width="{loaderSize()}" height="{loaderSize()}"
           x="{loaderPos().x}" y="{loaderPos().y}" class="loader-black" visible="{loading}"/>

</sprdSvg:PatternRendererClass>
