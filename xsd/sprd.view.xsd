    <complexType name="AppearanceColor">
        <complexContent>
            <extension base="ns:AppearanceColorClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.AppearanceColor"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="appearance"/>
                <attribute name="title" default="{appearance.name}"/>
                <attribute name="showTitle" default="true"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AppearanceColorClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.AppearanceColorClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="appearance"/>
                <attribute name="title" default="{appearance.name}"/>
                <attribute name="showTitle" default="true"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AppearanceColorPicker">
        <complexContent>
            <extension base="ns:AppearanceColorPickerClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.AppearanceColorPicker"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="appearance"/>
                <attribute name="showTitle" default="true"/>
                <attribute name="productType"/>
                <attribute name="product"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AppearanceColorPickerClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.AppearanceColorPickerClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="appearance"/>
                <attribute name="showTitle" default="true"/>
                <attribute name="productType"/>
                <attribute name="product"/>
                <attribute name="onappearanceSelect" default="_onAppearanceSelectHandler" type="string"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AppearanceImage">
        <complexContent>
            <extension base="ns:Image">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.AppearanceImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="appearance"/>
