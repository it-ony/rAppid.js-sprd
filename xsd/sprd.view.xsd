    <complexType name="DropDownSizeSelector">
        <complexContent>
            <extension base="ns:DropDownSizeSelectorClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.DropDownSizeSelector"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="type" default="{btnType()}"/>
                <attribute name="productType">
                    <annotation>
                        <documentation>
                        the product type to show sizes for
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="appearance">
                    <annotation>
                        <documentation>
                        the selected appearance of the product type
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="selectedSize">
                    <annotation>
                        <documentation>
                        the selected size
                    </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="DropDownSizeSelectorClass">
        <complexContent>
            <extension base="ns1:MenuButton">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.DropDownSizeSelectorClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="type" default="{btnType()}"/>
                <attribute name="productType">
                    <annotation>
                        <documentation>
                        the product type to show sizes for
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="appearance">
                    <annotation>
                        <documentation>
                        the selected appearance of the product type
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="selectedSize">
                    <annotation>
                        <documentation>
                        the selected size
                    </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="Image">
        <complexContent>
            <extension base="ns:ImageClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.Image"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="display" default="inline-block"/>
                <attribute name="loadLazy">
                    <annotation>
                        <documentation>
                        Set to true to load images only when in viewport
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="unveiled">
                    <annotation>
                        <documentation>
                        Returns true if image is unveiled
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="loaded">
                    <annotation>
                        <documentation>
                        the loading state
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="loadingError">
                    <annotation>
                        <documentation>
                        indicator if image has a loading error
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="width">
                    <annotation>
                        <documentation>
                        the width of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="height">
                    <annotation>
                        <documentation>
                        the height of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="src" default="{imageUrl()}">
                    <annotation>
                        <documentation>
                        the source of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="title">
                    <annotation>
                        <documentation>
                        the title of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="loadLazyThreshold">
                    <annotation>
                        <documentation>
                        Lazy Load threshold
                    </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ImageClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ImageClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="loadLazy">
                    <annotation>
                        <documentation>
                        Set to true to load images only when in viewport
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="unveiled">
                    <annotation>
                        <documentation>
                        Returns true if image is unveiled
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="loaded">
                    <annotation>
                        <documentation>
                        the loading state
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="loadingError">
                    <annotation>
                        <documentation>
                        indicator if image has a loading error
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="width">
                    <annotation>
                        <documentation>
                        the width of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="height">
                    <annotation>
                        <documentation>
                        the height of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="src" default="{imageUrl()}">
                    <annotation>
                        <documentation>
                        the source of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="title">
                    <annotation>
                        <documentation>
                        the title of the Image
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="loadLazyThreshold">
                    <annotation>
                        <documentation>
                        Lazy Load threshold
                    </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ImageUpload">
        <complexContent>
            <extension base="ns:ImageUploadClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ImageUpload"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="ondragenter" default="dragEnter"/>
                <attribute name="ondragexit" default="dragExit"/>
                <attribute name="ondrop" default="dropImage"/>
                <attribute name="ondragover" default="dragOver"/>
                <attribute name="items"/>
                <attribute name="imageWidth" default="100"/>
                <attribute name="displayNotice" default="true"/>
                <attribute name="uploadContext"/>
                <attribute name="notice" default="Drop your files here."/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ImageUploadClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ImageUploadClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="items"/>
                <attribute name="imageWidth" default="100"/>
                <attribute name="displayNotice" default="true"/>
                <attribute name="uploadContext"/>
                <attribute name="notice" default="Drop your files here."/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="Imprint">
        <complexContent>
            <extension base="ns:ImprintClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.Imprint"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="platform" default="EU"/>
                <attribute name="locale"/>
                <attribute name="name"/>
                <attribute name="defaults"/>
                <attribute name="street"/>
                <attribute name="city"/>
                <attribute name="country"/>
                <attribute name="board"/>
                <attribute name="boardDirector"/>
                <attribute name="vatId"/>
                <attribute name="tradeRegister"/>
                <attribute name="email"/>
                <attribute name="website"/>
                <attribute name="phone"/>
                <attribute name="fax"/>
                <attribute name="rstv"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ImprintClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ImprintClass">
                                 TODO: I18n.
                            </documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="platform" default="EU"/>
                <attribute name="locale"/>
                <attribute name="name"/>
                <attribute name="defaults"/>
                <attribute name="street"/>
                <attribute name="city"/>
                <attribute name="country"/>
                <attribute name="board"/>
                <attribute name="boardDirector"/>
                <attribute name="vatId"/>
                <attribute name="tradeRegister"/>
                <attribute name="email"/>
                <attribute name="website"/>
                <attribute name="phone"/>
                <attribute name="fax"/>
                <attribute name="rstv"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="MiniBasket">
        <complexContent>
            <extension base="ns:MiniBasketClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.MiniBasket"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="currency" default="{basket.currency}"/>
                <attribute name="basket" default="{basketManager.basket}"/>
                <attribute name="loading"/>
                <attribute name="itemCount" default="{basket.totalItemsCount()}"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="MiniBasketClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.MiniBasketClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="currency" default="{basket.currency}"/>
                <attribute name="basket" default="{basketManager.basket}"/>
                <attribute name="loading"/>
                <attribute name="itemCount" default="{basket.totalItemsCount()}"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="PrintColorImage">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.PrintColorImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="printColor"/>
                <attribute name="loaded"/>
                <attribute name="platform" default="{PARAMETER().platform}"/>
                <attribute name="data-print-color-id" default="{printColor.id}"/>
                <attribute name="backgroundColor" default="{backgroundColor()}"/>
                <attribute name="backgroundImage" default="{backgroundImage()}"/>
                <attribute name="backgroundPosition" default="{backgroundPosition()}"/>
                <attribute name="backgroundSize" default="34px"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="PrintTypePanelBase">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.PrintTypePanelBase"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="specialFlex"/>
                <attribute name="nylonFlex"/>
                <attribute name="plotType"/>
                <attribute name="configuration"/>
                <attribute name="possiblePrintTypes" default="{configuration.getPossiblePrintTypes(product.appearance)}"/>
                <attribute name="flex"/>
                <attribute name="flock"/>
                <attribute name="product"/>
                <attribute name="printType"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="PrintTypeSelector">
        <complexContent>
            <extension base="ns:PrintTypeSelectorClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.PrintTypeSelector"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="PrintTypeSelectorClass">
        <complexContent>
            <extension base="ns:PrintTypePanelBase">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.PrintTypeSelectorClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="onprintTypeSelect" default="_onPrintTypeSelectHandler" type="string"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProductImage">
        <complexContent>
            <extension base="ns:Image">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ProductImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="view">
                    <annotation>
                        <documentation>
                        the view of the corresponding product type. If the view isn't set
the default view will be used.
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="product" use="required">
                    <annotation>
                        <documentation>
                        the product to show

                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="appearance">
                    <annotation>
                        <documentation>
                        the appearance of the corresponding product type. If the appearance isn't
set the default appearance of the product is used.
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="type"/>
                <attribute name="mediaType"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProductTypeImage">
        <complexContent>
            <extension base="ns:Image">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ProductTypeImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="type" default="preview"/>
                <attribute name="productType"/>
                <attribute name="appearance"/>
                <attribute name="view"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProductTypeViewSelector">
        <complexContent>
            <extension base="ns:ProductTypeViewSelectorClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ProductTypeViewSelector"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="product">
                    <annotation>
                        <documentation>
                         if null use default view
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="appearance"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProductTypeViewSelectorClass">
        <complexContent>
            <extension base="ns1:SelectionView">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ProductTypeViewSelectorClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="product">
                    <annotation>
                        <documentation>
                         if null use default view
                    </documentation>
