        </complexContent>
    </complexType>
    <complexType name="ArticleImage">
        <complexContent>
            <extension base="ns:ProductImage">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ArticleImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ColorSelector">
        <complexContent>
            <extension base="ns:ColorSelectorClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ColorSelector"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="colorWidth" default="25"/>
                <attribute name="productType"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ColorSelectorClass">
        <complexContent>
            <extension base="ns1:SelectionView">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ColorSelectorClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="colorWidth" default="25"/>
                <attribute name="productType"/>
                <attribute name="onappearanceSelect" default="_onAppearanceSelectHandler" type="string"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ConfigurationColorSelector">
        <complexContent>
            <extension base="ns:ConfigurationColorSelectorClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ConfigurationColorSelector"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="configuration">
                    <annotation>
                        <documentation>
                        the configuration, to switch the color
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="layers" default="{layers()}"/>
                <attribute name="autoSelectLayer"/>
                <attribute name="selectedLayer"/>
                <attribute name="autoSelectConfigurationType"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ConfigurationColorSelectorClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.ConfigurationColorSelectorClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="configuration">
                    <annotation>
                        <documentation>
                        the configuration, to switch the color
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="layers" default="{layers()}"/>
                <attribute name="autoSelectLayer"/>
                <attribute name="selectedLayer"/>
                <attribute name="autoSelectConfigurationType"/>
                <attribute name="oncolorSelected" default="_onColorSelectedHandler" type="string"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="DesignImage">
        <complexContent>
            <extension base="ns:Image">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.DesignImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="design"/>
                <attribute name="view">
                    <annotation>
                        <documentation>
                         if null use default view
                    </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="DndImage">
        <complexContent>
            <extension base="ns:DndImageClass">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.DndImage"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="configurationViewer"/>
                <attribute name="svgContainer"/>
                <attribute name="design"/>
                <attribute name="hoverState"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="DndImageClass">
        <complexContent>
            <extension base="ns1:View">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.view.DndImageClass"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="configurationViewer"/>
                <attribute name="svgContainer"/>
                <attribute name="design"/>
