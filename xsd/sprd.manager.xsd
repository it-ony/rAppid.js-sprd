<schema targetNamespace="sprd.manager"
    xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:ns1="js.core"
    xmlns:ns="sprd.manager">
    <import/>
    <element name="ApiBasketManager" type="ns:ApiBasketManager"/>
    <element name="AuthenticationManager" type="ns:AuthenticationManager"/>
    <element name="DesignProductInitializer" type="ns:DesignProductInitializer"/>
    <element name="ExternalApi" type="ns:ExternalApi"/>
    <element name="ExternalBasketManager" type="ns:ExternalBasketManager"/>
    <element name="FeatureManager" type="ns:FeatureManager"/>
    <element name="IBasketManager" type="ns:IBasketManager"/>
    <element name="IErrorTrackingManager" type="ns:IErrorTrackingManager"/>
    <element name="IProductInitializer" type="ns:IProductInitializer"/>
    <element name="LocaleManager" type="ns:LocaleManager"/>
    <element name="OrderManager" type="ns:OrderManager"/>
    <element name="ProductCreationErrorManager" type="ns:ProductCreationErrorManager"/>
    <element name="ProductInitializer" type="ns:ProductInitializer"/>
    <element name="RaygunErrorTrackingManager" type="ns:RaygunErrorTrackingManager"/>
    <element name="TrackingManager" type="ns:TrackingManager"/>
    <element name="TrackjsErrorTrackingManager" type="ns:TrackjsErrorTrackingManager"/>
    <element name="VirtualProductManager" type="ns:VirtualProductManager"/>
    <complexType name="ApiBasketManager">
        <complexContent>
            <extension base="ns:IBasketManager">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.ApiBasketManager">
                                A BasketManager to interact with the Spreadshirt Basket API


                            </documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="basket">
                    <annotation>
                        <documentation>
                        the basket model
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="apiBasketId">
                    <annotation>
                        <documentation>
                        the id of the api basket which should try to be loaded.
If the id isn't set the id is retrieved from the local storage or generated by the api
after the basket has been created successfully. The id is written back to this property.
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="shop">
                    <annotation>
                        <documentation>
                        the shop used for creating a new basket
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="originId">
                    <annotation>
                        <documentation>
                        the origin id used for basked items
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="updating">
                    <annotation>
                        <documentation>
                        a flag that indicates if the basket is updating
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="initBasketWithNoCache" default="true"/>
                <attribute name="basketItem">
                    <annotation>
                        <documentation>
                        if set, the element which is added will be saved under this basket item
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="syncToOpossum">
                    <annotation>
                        <documentation>
                        a flag to trigger opossum synchronisation
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="onbasketChanged" default="_onBasketChangedHandler" type="string">
                    <annotation>
                        <documentation>
                        the basketChanged event is dispatched after the basket has been modified e.g. added, removed or updated an
BasketItem
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="onbasketUpdated" default="_onBasketUpdatedHandler" type="string"/>
                <attribute name="onbasketUpdating" default="_onBasketUpdatingHandler" type="string"/>
                <attribute name="onbasketSaving" default="_onBasketSavingHandler" type="string"/>
                <attribute name="onbasketInitialized" default="_onBasketInitializedHandler" type="string"/>
                <attribute name="oncouponApplied" default="_onCouponAppliedHandler" type="string"/>
                <attribute name="oncouponRemoved" default="_onCouponRemovedHandler" type="string"/>
                <attribute name="oncouponManualRemoved" default="_onCouponManualRemovedHandler" type="string"/>
                <attribute name="oncouponsLoaded" default="_onCouponsLoadedHandler" type="string"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AuthenticationManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.AuthenticationManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="session"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="DesignProductInitializer">
        <complexContent>
            <extension base="ns:IProductInitializer">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.DesignProductInitializer"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ExternalApi">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.ExternalApi"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ExternalBasketManager">
        <complexContent>
            <extension base="ns:IBasketManager">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.ExternalBasketManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="externalBasket"/>
                <attribute name="basketItem"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="FeatureManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.FeatureManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="calcInnerRects"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="IBasketManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.IBasketManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="continueShoppingLink">
                    <annotation>
                        <documentation>
                        continueShopping link is used in checkout as link for continue shopping button.
It will be automatically added to the element added to basket
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="editBasketItemLinkHook"/>
                <attribute name="editBasketItemUrl">
                    <annotation>
                        <documentation>
                        edit link is the link displayed in checkout for editing the basket item.
It will be automatically added to the element added to the basked.


                    </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="IErrorTrackingManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.IErrorTrackingManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="enabled" default="true"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="IProductInitializer">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.IProductInitializer"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="productManager"/>
                <attribute name="designConfigurationManager"/>
                <attribute name="textConfigurationManager"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="LocaleManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.LocaleManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="platform" default="EU"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="OrderManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.OrderManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="order">
                    <annotation>
                        <documentation>
                        the order model
                    </documentation>
                    </annotation>
                </attribute>
                <attribute name="shop"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProductCreationErrorManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.ProductCreationErrorManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="errorMessage"/>
                <attribute name="copyrightErrorMessage"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProductInitializer">
        <complexContent>
            <extension base="ns:DesignProductInitializer">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.ProductInitializer"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="RaygunErrorTrackingManager">
        <complexContent>
            <extension base="ns:IErrorTrackingManager">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.RaygunErrorTrackingManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="apiKey"/>
                <attribute name="beforeSendFnc"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="TrackingManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.TrackingManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="appName"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="TrackjsErrorTrackingManager">
        <complexContent>
            <extension base="ns:IErrorTrackingManager">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.TrackjsErrorTrackingManager"></documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="apiKey"/>
                <attribute name="application"/>
                <attribute name="ignoreMessages"/>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="VirtualProductManager">
        <complexContent>
            <extension base="ns1:Component">
                <annotation>
                    <documentation source="http://www.rappidjs.com/#/api/sprd.manager.VirtualProductManager">
                                 necessary dependency
                            </documentation>
                </annotation>
                <sequence>
                    <any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
                <attribute name="context"/>
                <attribute name="platform" default="EU"/>
            </extension>
        </complexContent>
    </complexType>
</schema>